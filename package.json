    "@types/yargs": "15.0.5",
    "ansi-regex": "^2.0.0",
    "body-parser": "1.19.0",
    "boxen": "4.2.0",
    "bump-version": "0.5.0",
    "chalk": "3.0.0",
    "chrome-remote-interface": "0.28.2",
    "chromedriver": "2.45.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.0.2",
