{
  "name": "oas",
  "version": "6.0.1027",
  "private": true,
  "packages": [
    "packages/web-lib-core",
    "packages/web-lib-common",
    "packages/web-lib-angular-js",
    "packages/web-lib-angular",
    "packages/web-app-prime",
    "packages/web-app-wui",
    "packages/web-app-edit",
    "packages/web-app-doc",
    "packages/web-app-structure",
    "packages/internal-lib-e2e"
  ],
  "scripts": {
    "full-install": "npm install && npm run all install -- --no-run && npm run symlink && npm run write-dependencies-hash",
    "check-for-updated-packages": "ts-node ./build-tools/checkForUpdatedPackages.ts",
    "write-dependencies-hash": "ts-node ./build-tools/writeDependenciesHash.ts",
    "symlink": "ts-node ./build-tools/symlink.ts",
    "symlink:force": "npm run symlink -- --force",
    "all": "ts-node ./build-tools/run-all/index.ts",
    "all:debug": "node -r ts-node/register --inspect-brk ./build-tools/run-all/index.ts",
    "ci": "npm run all ci -- --serial --exclude-missing",
    "build:prod": "npm run all build:prod -- --stages --exclude-missing",
    "build": "npm run build:prod",
    "dev": "npm run check-for-updated-packages && npm run all dev -- --stages --exclude web-app-doc web-app-structure --exclude-missing --doneCriteria \"Finished|Compilation complete\"",
    "dev:edit": "npm run check-for-updated-packages && npm run all dev -- --stages --include web-app-edit @oas/web-lib-angular @oas/web-lib-core --doneCriteria \"Finished|Compilation complete\"",
    "lint": "npm run all lint -- --exclude-missing",
    "lint:ts": "npm run all lint:ts -- --exclude-missing",
    "typecheck": "npm run all typecheck -- --exclude-missing",
    "serve": "npm run all serve  -- --exclude-missing",
    "serve:build": "npm run build:prod && npm run serve -- --exclude-missing",
    "compile": "npm run all compile -- --serial --exclude-missing",
    "clean": "npm run all clean --exclude-missing",
    "clean:dist": "npm run all clean:dist -- --exclude-missing",
    "clean:cache": "npm run all clean:cache -- --exclude-missing",
    "clean:testresults": "npm run all clean:testresults -- --exclude-missing",
    "webpack": "npm run all webpack",
    "webpack:debug": "npm run all webpack:debug",
    "test": "npm run all test -- --exclude-missing",
    "test:e2e": "npm run all test:e2e -- --exclude-missing",
    "test:e2e:headless": "npm run all test:e2e:headless -- --exclude-missing",
    "test:integration": "npm run all test:integration -- --exclude-missing",
    "test:integration:ci": "npm run all test:integration:ci -- --exclude-missing",
    "protractor": "npm run all protractor -- --serial --exclude-missing",
    "reset": "ts-node build-tools/reset.ts && npm run full-install",
    "bump-version": "bump",
    "check-for-fdescribe": "ts-node ./build-tools/checkForForbiddenE2ECommands.ts",
    "ng:version": "ng version",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check-for-fdescribe && run-p lint typecheck",
      "pre-push": "ts-node ./build-tools/pre-push"
    }
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "0.1000.0",
    "@angular/cli": "10.0.0",
    "@angular/compiler-cli": "10.0.1",
    "@ngtools/webpack": "10.0.0",
    "@types/core-js": "2.5.3",
    "@types/glob": "7.1.2",
    "@types/jest": "26.0.3",
