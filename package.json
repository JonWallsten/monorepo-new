{
  "name": "oas",
  "version": "6.0.1027",
  "private": true,
  "packages": [
    "packages/web-lib-core",
    "packages/web-lib-common",
    "packages/web-lib-angular-js",
    "packages/web-lib-angular",
    "packages/web-app-prime",
    "packages/web-app-wui",
    "packages/web-app-edit",
    "packages/web-app-doc",
    "packages/web-app-structure",
    "packages/internal-lib-e2e"
  ],
  "scripts": {
    "full-install": "npm install && npm run all install -- --no-run && npm run symlink && npm run write-dependencies-hash",
    "prune": "npm run all prune -- --no-run",
    "check-for-updated-packages": "ts-node ./build-tools/checkForUpdatedPackages.ts",
    "write-dependencies-hash": "ts-node ./build-tools/writeDependenciesHash.ts",
    "symlink": "ts-node ./build-tools/symlink.ts",
    "symlink:force": "npm run symlink -- --force",
    "mirror-version": "ts-node ./ci/mirror-version.ts",
    "kill-node": "ts-node ./build-tools/kill-node.ts",
    "all": "ts-node ./build-tools/run-all/index.ts",
    "all:debug": "node -r ts-node/register --inspect-brk ./build-tools/run-all/index.ts",
    "publish-all": "npm run all publish -- --serial --no-run",
    "ci": "npm run all ci -- --serial --exclude-missing",
    "build:lib:dev": "npm run all build:dev -- --stages --include @oas/web-lib-* @oas/internal-lib-* --exclude-missing",
    "build:lib:prod": "npm run all build:prod -- --stages --include @oas/web-lib-* @oas/internal-lib-*",
    "build:edit": "npm run check-for-updated-packages && npm run all build -- --stages --include web-app-edit @oas/web-lib-angular @oas/web-lib-core",
    "build:dev": "npm run build:lib:dev && npm run all build:dev -- --exclude @oas/web-lib-* @oas/internal-lib-* --exclude-missing",
    "build:prod": "npm run all build:prod -- --stages --exclude-missing",
    "build": "npm run build:prod",
    "build:sm": "cross-env-shell USE_SOURCEMAPS=true npm run build:prod",
    "dev:nocheck": "npm run build:lib:dev && npm run all dev -- --exclude web-app-doc web-app-structure --exclude-missing",
    "dev": "npm run check-for-updated-packages && npm run build:lib:dev && npm run all dev -- --exclude web-app-doc web-app-structure --exclude-missing",
    "dev:fast": "npm run all dev -- --exclude web-app-doc web-app-structure --exclude-missing",
    "dev:all": "npm run check-for-updated-packages && npm run build:lib:dev && npm run all dev",
    "dev:init": "npm run dev",
    "dev:edit": "npm run check-for-updated-packages && npm run all dev -- --include web-app-edit @oas/web-lib-angular @oas/web-lib-core",
    "dev:edit:prime": "npm run check-for-updated-packages && npm run all dev -- --include web-app-prime web-app-edit @oas/web-lib-core @oas/web-lib-core",
    "dev:wui": "npm run check-for-updated-packages && npm run all dev -- --include web-app-wui @oas/web-lib-angular-js @oas/web-lib-core",
    "dev:clean": "npm run all clean && npm run dev:init",
    "start": "npm run dev:init",
    "lint:ts": "npm run all lint:ts -- --exclude-missing",
    "lint:ts:fix": "npm run all lint:ts:fix -- --exclude-missing",
    "lint:js": "npm run all lint:js -- --exclude-missing",
    "lint:style": "npm run all lint:style -- --exclude-missing",
    "lint:html": "npm run all lint:html -- --exclude-missing",
    "lint:bootstrap": "npm run all lint:bootstrap -- --exclude-missing",
    "lint": "npm run all lint -- --exclude-missing",
    "typecheck": "npm run all typecheck -- --exclude-missing",
    "serve": "npm run all serve  -- --exclude-missing",
    "serve:test": "npm run all serve:test -- --exclude-missing",
    "serve:build": "npm run serve:build:prod",
    "serve:build:prod": "npm run build:prod && npm run serve -- --exclude-missing",
    "compile": "npm run all compile -- --serial --exclude-missing",
    "clean": "npm run all clean --exclude-missing",
    "clean:dist": "npm run all clean:dist -- --exclude-missing",
    "clean:cache": "npm run all clean:cache -- --exclude-missing",
    "clean:testresults": "npm run all clean:testresults -- --exclude-missing",
    "webpack": "npm run all webpack",
    "webpack:debug": "npm run all webpack:debug",
    "test": "npm run all test -- --exclude-missing",
    "test:e2e": "npm run all test:e2e -- --exclude-missing",
    "test:e2e:headless": "npm run all test:e2e:headless -- --exclude-missing",
    "test:integration": "npm run all test:integration -- --exclude-missing",
    "test:integration:ci": "npm run all test:integration:ci -- --exclude-missing",
    "protractor": "npm run all protractor -- --serial --exclude-missing",
    "reset": "ts-node build-tools/reset.ts && npm run full-install",
    "bump-version": "bump",
    "check-for-fdescribe": "ts-node ./build-tools/checkForForbiddenE2ECommands.ts",
    "ng:version": "ng version",
    "postinstall": "ngcc --properties es5 browser module main --first-only --create-ivy-entry-points"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check-for-fdescribe && run-p lint typecheck",
      "pre-push": "ts-node ./build-tools/pre-push"
    }
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "0.900.1",
    "@angular/cli": "9.0.1",
    "@angular/compiler-cli": "9.0.0",
    "@ngtools/webpack": "9.0.1",
    "@types/core-js": "2.5.2",
    "@types/glob": "7.1.1",
    "@types/jest": "24.0.25",
    "@types/jquery": "2.x.x",
    "@types/lodash": "4.14.149",
    "@types/node": "12.12.6",
    "@types/promise.prototype.finally": "2.0.3",
    "@types/split": "1.0.0",
    "@types/webpack": "4.41.2",
    "@types/webpack-env": "1.15.0",
    "@types/yargs": "15.0.0",
    "ansi-regex": "^2.0.0",
    "body-parser": "1.19.0",
    "boxen": "4.2.0",
    "bump-version": "0.5.0",
    "chalk": "3.0.0",
    "chrome-remote-interface": "0.28.1",
    "chromedriver": "2.45.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.1.1",
    "core-js": "3.6.4",
    "cross-env": "6.0.3",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "eslint-plugin-angular": "4.0.1",
    "express": "4.17.1",
    "file-loader": "5.0.2",
    "gelf": "2.0.1",
    "glob": "7.1.6",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "htmlhint": "0.11.0",
    "husky": "4.0.3",
    "image-size": "0.8.3",
    "ip": "1.1.5",
    "jasmine-console-reporter": "^2.0.1",
    "jasmine-reporters": "2.3.2",
    "jest": "24.9.0",
    "jest-zone-patch": "0.0.10",
    "less": "3.10.3",
    "less-loader": "5.0.0",
    "linebyline": "1.3.0",
    "mini-css-extract-plugin": "0.9.0",
    "minimist": "1.2.0",
    "ncp": "2.0.0",
    "node-object-hash": "2.0.0",
    "node-sass": "4.12.0",
    "npm-run-all": "4.1.5",
    "postcss": "7.0.26",
    "postcss-easy-import": "3.0.0",
    "postcss-load-plugins": "2.3.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "protractor": "5.4.2",
    "puppeteer": "1.9.0",
    "recursive-copy": "2.0.10",
    "replace-in-file": "5.0.2",
    "rimraf": "3.0.0",
    "sass-loader": "8.0.2",
    "source-map-loader": "0.2.4",
    "split": "1.0.1",
    "strip-ansi": "^3.0.0",
    "style-loader": "1.1.3",
