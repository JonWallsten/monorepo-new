{
  "name": "web-app-wui",
  "version": "0.0.0-PLACEHOLDER",
  "private": true,
  "engines": {
    "node": "8.10.0"
  },
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "scripts": {
    "ci": "npm run clean && npm run lint && npm run test && npm run build:prod",
    "build:dev": "npm run webpack -- --config config/webpack.dev.ts --colors --hide-modules --display-error-details --profile --display-entrypoints",
    "build:prod": "npm run webpack -- --config config/webpack.prod.ts --bail",
    "build": "npm run clean && npm run build:prod",
    "dev": "npm run webpack-dev-server -- --config config/webpack.dev.ts --colors --hot",
    "start": "npm run dev",
    "lint:ts": "tslint -c tslint.json src/**/*.ts",
    "lint:ts:fix": "tslint --fix -t codeFrame 'src/**/*.ts'",
    "lint:js": "eslint --max-warnings 0 ./src/**/*.js",
    "lint": "npm run lint:ts && npm run lint:js",
    "serve": "cross-env IS_LOCAL=true ts-node ../../ci/serve",
    "serve:build": "npm run build:prod && npm run serve",
    "compile": "npm run clean:cache && tsc",
    "clean": "npm run clean:dist && npm run clean:cache",
    "clean:dist": "rimraf dist",
    "clean:cache": "rimraf .tmp",
    "clean:testresults": "rimraf .test_results",
    "webpack:debug": "ts-node --inspect --debug-brk --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
    "unlink": "rimraf node_modules/@oas/web-lib-angular-js node_modules/@oas/web-lib-core",
    "test:e2e": "npm run clean:testresults && cross-env frontendServer=http://localhost:3010 IS_DEV=true envId=10 npm run protractor",
    "test:e2e:headless": "cross-env-shell HEADLESS=true npm run test:e2e",
    "test:e2e:install": "node ./ci/test-install.js && npm install",
    "webpack": "node --max_old_space_size=8192 ../../node_modules/webpack/bin/webpack.js",
    "webpack-dev-server": "node --max_old_space_size=8192 ../../node_modules/webpack-dev-server/bin/webpack-dev-server",
    "protractor": "cross-env TS_NODE_PROJECT=tsconfig.build.json node -r ts-node/register -r tsconfig-paths/register ../../node_modules/protractor/bin/protractor protractor.conf.ts",
    "protractor:nightly": "cross-env TS_NODE_PROJECT=tsconfig.e2e.nightly.json node -r ts-node/register -r tsconfig-paths/register node_modules/protractor/bin/protractor protractor.conf.ts"
  },
  "devDependencies": {
    "@types/angular": "1.5.23",
    "@types/angular-mocks": "1.5.11",
    "@types/angular-ui-router": "1.1.40",
    "@types/jquery": "2.x.x",
    "chai": "4.2.0",
    "chai-subset": "1.6.0",
    "monaco-editor": "0.19.0",
    "monaco-editor-webpack-plugin": "1.8.1",
    "ng-annotate-loader": "0.6.1"
  },
  "dependencies": {
    "angular": "1.5.x",
    "angular-diff-match-patch": "0.6.10",
    "angular-messages": "1.5.x",
    "angular-mocks": "1.5.x",
    "angular-sanitize": "1.5.x",
    "angular-translate": "2.18.1",
    "angular-ui-ace": "0.2.x",
    "angular-ui-bootstrap": "1.3.x",
    "angular-ui-router": "0.3.x",
    "bootstrap": "3.3.7",
    "ng-file-upload": "9.1.2",
    "ui-select": "0.19.x"
  },
  "internalDependencies": {
    "@oas/web-lib-core": "0.0.0",
    "@oas/web-lib-common": "0.0.0",
    "@oas/web-lib-angular-js": "0.0.0"
  }
}
