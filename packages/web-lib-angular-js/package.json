{
  "name": "@oas/web-lib-angular-js",
  "version": "0.0.0-PLACEHOLDER",
  "engines": {
    "node": "^12.14.0"
  },
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "ci": "npm run clean && npm run lint && npm run test && npm run build:prod",
    "build:dev": "npm run webpack -- --config config/webpack.dev.ts --display minimal --display-error-details",
    "build:dev:watch": "npm run build:dev -- --watch",
    "build:prod": "npm run webpack -- --config config/webpack.prod.ts --display errors-only --display-error-details",
    "build": "npm run clean && npm run build:prod",
    "dev": "npm run build:dev:watch",
    "lint:ts": "tslint -c tslint.json src/**/*.ts",
    "lint:ts:fix": "tslint --fix -t codeFrame 'src/**/*.ts'",
    "lint:js": "eslint --max-warnings 0 ./src/**/*.js",
    "lint": "npm run lint:ts && npm run lint:js",
    "test": "npm run test:unit",
    "test:unit": "node -r ts-node/register ../../node_modules/jest/bin/jest",
    "test:unit:watch": "node -r ts-node/register ../../node_modules/jest/bin/jest --watch",
    "test:unit:debug": "node --inspect --debug-brk ../../node_modules/jest/bin/jest",
    "compile": "npm run clean:cache && tsc",
    "clean": "npm run clean:dist && npm run clean:cache",
    "clean:dist": "rimraf -- dist",
    "clean:cache": "rimraf -- .tmp",
    "webpack": "node --max_old_space_size=8192 ../../node_modules/webpack/bin/webpack.js",
    "webpack:debug": "node --inspect-brk --max_old_space_size=8192 ../../node_modules/webpack/bin/webpack.js"
  },
  "devDependencies": {
    "@types/angular": "1.5.23",
    "@types/angular-mocks": "1.5.11",
    "@types/angular-ui-router": "1.1.40",
    "lodash-ts-imports-loader": "1.2.0",
    "ng-annotate-loader": "0.6.1"
  },
  "dependencies": {
    "angular": "1.5.x",
    "bootstrap": "3.3.7"
  },
  "peerDependencies": {
    "@oas/web-lib-core": "x.x.x",
    "@oas/web-lib-common": "x.x.x",
    "angular": "1.5.x",
    "angular-diff-match-patch": "0.6.10",
    "angular-messages": "1.5.x",
    "angular-mocks": "1.5.x",
    "angular-sanitize": "1.5.x",
    "angular-translate": "2.x.x",
    "angular-ui-ace": "0.2.x",
    "angular-ui-bootstrap": "1.3.x",
    "angular-ui-router": "0.3.x",
    "bootstrap": "3.3.7",
    "ng-file-upload": "9.1.2",
    "ui-select": "0.19.x"
  },
  "internalDependencies": {
    "@oas/web-lib-core": "0.0.0",
    "@oas/web-lib-common": "0.0.0"
  }
}
